<h5><i class="icon-user"></i> People</h5>
<% for user in @neighborhood.users %>
    <%= image_tag user.profile_picture, class: "med-thumbnail" %>
<% end %>
<h5><i class="icon-map-marker"></i> Places</h5>
  <% for place in @neighborhood.places %>
    <div class="place-teaser">
        <%= image_tag(place.photos.first.image.url(:small), class: "mini-thumbnail") unless place.photos.empty? %>
        <div class="place-teaser-details">
          <span class="pt-title"><%= place.name %></span><br />
          <span class="stats"><i class='icon-comment'></i> <%= place.posts.count %> Reviews</span>
          <span class="stats"><i class='icon-heart'></i> <%= place.favorites.count %> Favorites</span>
        </div>
      </div>
  <% end %>
